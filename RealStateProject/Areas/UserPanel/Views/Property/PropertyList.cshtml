                @Html.DisplayFor(modelItem => item.HomeProperty_Type.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rigion.RegionTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubUsage.SubUsageTitle)
            </td>
            <td>
