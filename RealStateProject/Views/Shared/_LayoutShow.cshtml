@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home Property | Properties</title>
    @if (System.Globalization.CultureInfo.CurrentCulture.Name.Contains("fa"))
    {

        <link href="/Content/bootstrap-rtl.css" rel="stylesheet" />
        <link href="/assets-fa/css/style.css" rel="stylesheet">
        <link href="/assets-fa/css/font-awesome.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="/assets-fa/css/slick.css">
        <link rel="stylesheet" type="text/css" href="/assets-fa/css/nouislider.css">
        <link rel="stylesheet" href="/assets-fa/css/jquery.fancybox.css" type="text/css" media="screen" />
        <link id="switcher" href="/assets-fa/css/theme-color/default-theme.css" rel="stylesheet">
    }
    else
    {

        <link href="/assets-en/css/bootstrap.css" rel="stylesheet" />
        <link href="/assets-en/css/style.css" rel="stylesheet">
        <link href="/assets-en/css/font-awesome.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="/assets-en/css/slick.css">
        <link rel="stylesheet" type="text/css" href="/assets-en/css/nouislider.css">
        <link rel="stylesheet" href="/assets-en/css/jquery.fancybox.css" type="text/css" media="screen" />
        <link id="switcher" href="/assets-en/css/theme-color/default-theme.css" rel="stylesheet">
    }



</head>
<body class="aa-price-range">
    <!-- Pre Loader -->
    <div id="aa-preloader-area">
        <div class="pulse"></div>
    </div>
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->
    <!-- Start header section -->
    <header id="aa-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-header-area">
                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div class="aa-header-right">
                                    @Html.Partial("_LogginUser")
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div class="aa-header-left">
                                    <div class="aa-telephone-no">
                                        <span class="fa fa-phone"></span>
                                        1-900-523-3564
                                    </div>
                                    <div class="aa-email hidden-xs">
                                        <span class="fa fa-envelope-o"></span> info@markups.com
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- End header section -->
    <!-- Start menu section -->
    <section id="aa-menu-area">
        <nav class="navbar navbar-default main-navbar" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- LOGO -->
                    <!-- Text based logo -->
                    @*<a class="navbar-brand aa-logo" href="index.html"> خانه <span>لورم ایپسوم</span></a>*@
                    <!-- Image based logo -->
                    <!-- <a class="navbar-brand aa-logo-img" href="index.html"><img src="img/logo.png" alt="logo"></a> -->
                </div>
                <div id="navbar" class="collapse navbar-collapse pull-left">
                    <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
                        <li class="active"><a href="Home">@DataLayer.Resources.Resource_Main.Home</a></li>
                        @Html.Partial("_LanguageSetting")
                        @*<li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="properties.html">ویژگی ها <span class="caret"></span></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="properties.html">جزئیات</a></li>
                                    <li><a href="properties-detail.html">جزئیات ویژگی ها</a></li>
                                </ul>
                            </li>
                            <li><a href="gallery.html">گالری</a></li>
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="blog-archive.html">وبلاگ <span class="caret"></span></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="blog-archive.html">جزئیات</a></li>
                                    <li><a href="blog-single.html">جزئیات بلاگ</a></li>
                                </ul>
                            </li>*@
                        <li><a href="contact.html">@DataLayer.Resources.Resource_Main.Contact</a></li>

                    </ul>
                </div><!--/.nav-collapse -->
            </div>
        </nav>
    </section>
    <!-- End menu section -->
    <!-- Start Proerty header  -->
    @*---------------------------------------------------------این قسمت فرق داره<*@
    <section id="aa-property-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-property-header-inner">
                        <h2>نمایش همه </h2>
                        <ol class="breadcrumb">
                            <li><a href="#">خانه</a></li>
                            <li class="active">آکهی</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Proerty header  -->
    <!-- Start Properties  -->
    <section id="aa-properties">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="aa-properties-content">
                        <!-- start properties content head -->
                       
                        <!-- Start properties content body -->
                        @RenderBody()

                        <!-- Start properties content bottom -->
                       
                    </div>
                </div>
                <!-- Start properties sidebar -->
                <div class="col-md-4">
                    <aside class="aa-properties-sidebar">
                        <!-- Start Single properties sidebar -->
                        <div class="aa-properties-single-sidebar">
                            <h3>جزییات جستجو</h3>
                            <form action="">
                                <div class="aa-single-advance-search">
                                    <input type="text" placeholder="محدوده خود را وارد کنید">
                                </div>
                                <div class="aa-single-advance-search">
                                    <select>
                                        <option value="0" selected>@DataLayer.Resources.Resource_Main.category</option>
                                        <option value="1">یک دست</option>
                                        <option value="2">زمین</option>
                                        <option value="3">قطعه</option>
                                        <option value="4">تجاری</option>
                                    </select>
                                </div>
                                
                                <div class="aa-single-advance-search">
                                    <select>
                                        <option value="0" selected>@DataLayer.Resources.Resource_Main.HomeType</option>
                                        <option value="1">یک دست</option>
                                        <option value="2">زمین</option>
                                        <option value="3">قطعه</option>
                                        <option value="4">تجاری</option>
                                    </select>
                                </div>

                                <div class="aa-single-advance-search">
                                    <select>
                                        <option value="0" selected>@DataLayer.Resources.Resource_Main.DealType</option>
                                        <option value="1">یک دست</option>
                                        <option value="2">زمین</option>
                                        <option value="3">قطعه</option>
                                        <option value="4">تجاری</option>
                                    </select>
                                </div>
                                <div class="">

                                    <div class="aa-single-filter-search">
                                        <span>@DataLayer.Resources.Resource_Main.Price</span>
                                        <span>@DataLayer.Resources.Resource_Main.From</span>
                                        <span id="skip-value-lower2" class="example-val">30.00</span>
                                        <span>@DataLayer.Resources.Resource_Main.To</span>
                                        <span id="skip-value-upper2" class="example-val">100.00</span>
                                        <div id="aa-price-range" class="noUi-target noUi-ltr noUi-horizontal noUi-background">
                                        </div>
                                    </div>
                                    <div class="aa-single-filter-search">
                                        <span>@DataLayer.Resources.Resource_Main.Area</span>
                                        <span>@DataLayer.Resources.Resource_Main.From</span>
                                        <span id="skip-value-lower" class="example-val">30.00</span>
                                        <span>@DataLayer.Resources.Resource_Main.To</span>
                                        <span id="skip-value-upper" class="example-val">100.00</span>
                                        <div id="aa-sqrfeet-range" class="noUi-target noUi-ltr noUi-horizontal noUi-background">
                                        </div>
                                    </div>




                                </div>


                                <div class="aa-single-advance-search">
                                    <input class="aa-search-btn" type="submit" value="@DataLayer.Resources.Resource_Main.Search">
                                </div>
                            </form>
                        </div>
                        <!-- Start Single properties sidebar -->
                        <div class="aa-properties-single-sidebar">
                            <h3>پربازدیدترین ها</h3>
                            
                            @Html.Action("PapulerProperty")
                            @*@RenderSection("PapulerProperty", false)*@
                        </div>
                    </aside>
                </div>
            </div>
        </div>
    </section>
    <!-- / Properties  -->
    <!-- Footer -->
    <footer id="aa-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="aa-footer-area">
                        <div class="row">
                            <div class="col-md-3 col-sm-6 col-xs-12">
                                <div class="aa-footer-left">
                                    <p>Designed by <a rel="nofollow" href="http://www.markups.io/">MarkUps.io</a></p>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-6 col-xs-12">
                                <div class="aa-footer-middle">
                                    <a href="#"><i class="fa fa-facebook"></i></a>
                                    <a href="#"><i class="fa fa-twitter"></i></a>
                                    <a href="#"><i class="fa fa-google-plus"></i></a>
                                    <a href="#"><i class="fa fa-youtube"></i></a>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12 col-xs-12">
                                <div class="aa-footer-right">
                                    <a href="#">Home</a>
                                    <a href="#">Support</a>
                                    <a href="#">License</a>
                                    <a href="#">FAQ</a>
                                    <a href="#">Privacy & Term</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- / Footer -->
    <!-- jQuery library -->
    <script src="~/assets-fa/js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/assets-fa/js/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="~/assets-fa/js/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="~/assets-fa/js/nouislider.js"></script>
    <!-- mixit slider -->
    <script type="text/javascript" src="~/assets-fa/js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="~/assets-fa/js/jquery.fancybox.pack.js"></script>
    <!-- Custom js -->
    <script src="~/assets-fa/js/custom.js"></script>

</body>
</html>




